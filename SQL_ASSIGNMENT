C-004 RAMESH                         +91-923-523-4534
C-003 SUNITHA                        +91-943-897-8389
C-002 JAMES                          +91-923-789-3481
C-001 JOHN                           +91-973-452-6719



21)  SELECT ACCOUNT_NO, ACCOUNT_TYPE, CUSTOMER_ID, CUSTOMER_NAME,DATE_OF_BIRTH, GUARDIAN_NAME, CONTACT_NO, MAIL_ID, GENDER,REFERENCE_ACC_NAME,REFERENCE_ACC_NO, REGISTRATION_DATE, ACTIVATION_DATE, (ACTIVATION_DATE-REGISTRATION_DATE) AS NoofdaysforActivation,
BANK_NAME, BRANCH_NAME, INITIAL_DEPOSIT
FROM CUSTOMER_PERSONAL_INFO NATURAL JOIN ACCOUNT_INFO NATURAL JOIN BANK_INFO BI NATURAL JOIN CUSTOMER_REFERENCE_INFO;


