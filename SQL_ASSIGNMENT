16) SELECT CUSTOMER_ID, CUSTOMER_NAME, ACCOUNT_NO, BANK_NAME, BRANCH_NAME, CONTACT_NO, MAIL_ID
FROM CUSTOMER_PERSONAL_INFO NATURAL JOIN ACCOUNT_INFO NATURAL JOIN BANK_INFO 
