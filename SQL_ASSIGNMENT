17)SELECT CUSTOMER_ID, BANK_NAME, BRANCH_NAME, IFSC_CODE, REGISTRATION_DATE, ACTIVATION_DATE
FROM CUSTOMER_PERSONAL_INFO NATURAL JOIN ACCOUNT_INFONATURAL JOIN BANK_INFO
WHERE EXTRACT(MONTH FROM DATE ACTIVATION_DATE)='MAR';





18) SELECT ((INTEREST/100)*INITIAL_DEPOSIT) AS INTEREST_AMT, CUSTOMER_ID, CUSTOMER_NAME, ACCOUNT_NO, ACCOUNT_TYPE, INTEREST, INITIAL_DEPOSIT
FROM CUSTOMER_PERSONAL_INFO NATURAL JOIN ACCOUNT_INFO ;


