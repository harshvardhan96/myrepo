2)  SELECT CUSTOMER_ID, ACCOUNT_TYPE, ACCOUNT_NO
	FROM BANK_INFO NATURAL JOIN ACCOUNT_INFO
	WHERE BANK_NAME ='HDFC' AND REGISTRATION_DATE BETWEEN '12-JAN-2012' AND '04-APR-2012';


	CUSTO ACCOUNT_TY ACCOUNT_NO
