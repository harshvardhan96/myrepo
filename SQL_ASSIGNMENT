20)  SELECT CUSTOMER_ID, CUSTOMER_NAME, CONCAT(CONCAT(CONCAT(CONCAT(CONCAT('+91-',SUBSTR(CONTACT_NO,1,3)),'-'),SUBSTR(CONTACT_NO,4,3)),'-'),SUBSTR(CONTACT_NO,-4)) AS CONTACT_ISD
FROM CUSTOMER_PERSONAL_INFO
ORDER BY CUSTOMER_ID DESC;

