3) SELECT CUSTOMER_ID, CUSTOMER_NAME, ACCOUNT_NO, ACCOUNT_TYPE, BANK_NAME
FROM BANK_INFO NATURAL JOIN ACCOUNT_INFO NATURAL JOIN CUSTOMER_PERSONAL_INFO;

