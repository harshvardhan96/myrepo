11) SELECT CUSTOMER_ID, CUSTOMER_NAME, ACCOUNT_NO, ACCOUNT_TYPE, ACTIVATION_DATE, BANK_NAME
FROM CUSTOMER_PERSONAL_INFO NATURAL JOIN ACCOUNT_INFO NATURAL JOIN BANK_INFO
WHERE ACTIVATION_DATE='10-APR-2012';

