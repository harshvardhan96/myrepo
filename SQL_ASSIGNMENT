22) SELECT CUSTOMER_ID, CUSTOMER_NAME, GUARDIAN_NAME, IDENTIFICATION_DOC_TYPE,REFERENCE_ACC_NAME, ACCOUNT_TYPE, IFSC_CODE, BANK_NAME, (INITIAL_DEPOSIT+INTEREST) AS CURRENT_BALANCE
FROM CUSTOMER_PERSONAL_INFO INNER JOIN ACCOUNT_INFO INNER JOIN BANK_INFO INNER JOIN CUSTOMER_REFERENCE_INFO
WHERE ACCOUNT_TYPE='SAVINGS';



23) SELECT CUSTOMER_ID, CUSTOMER_NAME, ACCOUNT_NO, ACCOUNT_TYPE, INTEREST, INITIAL_DEPOSIT,
CASE
WHEN INITIAL_DEPOSIT=20000 THEN 'high'
WHEN INITIAL_DEPOSIT=16000 THEN 'moderate'
WHEN INITIAL_DEPOSIT=10000 THEN 'average'
WHEN INITIAL_DEPOSIT=5000 THEN 'low'
WHEN INITIAL_DEPOSIT=0 THEN 'very low'
ELSE 'invalid' END DEPOSIT_STATUS
FROM CUSTOMER_PERSONAL_INFO NATURAL JOIN ACCOUNT_INFO;


CUSTO CUSTOMER_NAME                  ACCOUNT_NO ACCOUNT_TY   INTEREST INITIAL_DEPOSIT DEPOSIT_
----- ------------------------------ ---------- ---------- ---------- --------------- --------
C-001 JOHN                           1.2346E+15 SAVINGS             5           10000 average
C-002 JAMES                          1.2346E+15 SALARY              6               0 very low
C-003 SUNITHA                        1.2346E+15 SAVINGS             4           16000 moderate
C-004 RAMESH                         1.2346E+15 SALARY              7               0 very low
C-005 KUMAR                          1.2346E+15 SAVINGS             8           20000 high



24) SELECT CUSTOMER_ID, CUSTOMER_NAME, ACCOUNT_NO, ACCOUNT_TYPE, BANK_NAME, IFSC_CODE, INITIAL_DEPOSIT,
CASE
IF ACCOUNT_TYPE='SAVINGS' THEN ROUND((INTEREST+(INTEREST*10/100)),2)
ELSE INTEREST END 
FROM CUSTOMER_PERSONAL_INFO NATURAL JOIN ACCOUNT_INFO NATURAL JOIN BANK_INFO 
WHERE CUSTOMER_NAME LIKE 'J%';



25)
 SELECT CUSTOMER_ID, CUSTOMER_NAME, ACCOUNT_NO, INITIAL_DEPOSIT,
CASE 
WHEN INITIAL_DEPOSIT = 0 THEN '0%'
WHEN INITIAL_DEPOSIT<=10000 THEN '3%'
WHEN INITIAL_DEPOSIT>10000 AND INITIAL_DEPOSIT<20000 THEN '5%'
WHEN INITIAL_DEPOSIT>=20000 AND INITIAL_DEPOSIT<=30000 THEN '7%'
WHEN INITIAL_DEPOSIT>30000 THEN '10%' END 'TAX PERCENTAGE'
FROM CUSTOMER_PERSONAL_INFO NATURAL JOIN ACCOUNT_INFO ;